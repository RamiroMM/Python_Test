pipeline {
  agent any
  stages {
    stage('Init'){
      steps{
        echo 'Hi this is Ramiro Mora, from Mexico'
        echo 'This is a new test...'
      }
    }
    stage('Build'){
      steps{
        echo 'Building the sample project'
      }
    }
    stage('Deploy'){
      steps{
        echo 'Deploying to staging'
      }
    }
    stage('Deploy to Prod'){
      steps{
        echo 'Deploying to Production'
      }
    }
  }
}
